{
    "name": "sunaoka/rsync-utils",
    "description": "rsync utilities for php",
    "keywords": ["rsync", "--itemize-changes"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\RsyncUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\RsyncUtils\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
